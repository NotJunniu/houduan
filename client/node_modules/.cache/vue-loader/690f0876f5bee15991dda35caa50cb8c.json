                       <div>\r\n                        <span class=\"name\">\r\n                            出发地：\r\n                        </span>\r\n                        <span class=\"val\">\r\n                            {{ map.chufadi }}                        </span>\r\n                    </div>\r\n                                    <div>\r\n                        <span class=\"name\">\r\n                            途经地：\r\n                        </span>\r\n                        <span class=\"val\">\r\n                            {{ map.tujingdi }}                        </span>\r\n                    </div>\r\n                                    <div>\r\n                        <span class=\"name\">\r\n                            终点：\r\n                        </span>\r\n                        <span class=\"val\">\r\n                            {{ map.zhongdian }}                        </span>\r\n                    </div>\r\n                                    <div>\r\n                        <span class=\"name\">\r\n                            价格：\r\n                        </span>\r\n                        <span class=\"val\">\r\n                            {{ map.jiage }}                        </span>\r\n                    </div>\r\n                                    <div>\r\n                        <span class=\"name\">\r\n                            浏览量：\r\n                        </span>\r\n                        <span class=\"val\">\r\n                            {{ map.liulanliang }}                        </span>\r\n                    </div>\r\n                            </div>\r\n\r\n                            <el-button type=\"primary\" @click=\"$router.push('/yudingadd?id='+map.id)\">预定</el-button>\r\n            \r\n                            <e-collect v-model=\"isCollect\"\r\n                           module=\"lvyouxianlu\"\r\n                           ziduan=\"xianlumingcheng\"\r\n                           :id=\"map.id\"></e-collect>\r\n            \r\n        </div>\r\n    </div>\r\n    <div class=\"goods-content\">\r\n            </div>\r\n</div>\r\n\r\n</e-module-model-box>\r\n\r\n</div>    \r\n\r\n</e-container>\r\n</div>\r\n<div>\r\n<e-container>\r\n            <div  style=\"margin:15px 0 15px 0;background-color:#FFFFFF\">\r\n\r\n\r\n<e-container>\r\n    <el-tabs type=\"border-card\">\r\n                <el-tab-pane label=\"线路特色\">\r\n            <div  style=\"margin:15px 0 15px 0;background-color:#FFFFFF\">\r\n\r\n<div style=\"text-indent: 2em\">\r\n<div v-html=\"map.xianlutese\"></div></div>\r\n\r\n</div>        </el-tab-pane>\r\n                <el-tab-pane label=\"线路简介\">\r\n            <div  style=\"margin:15px 0 15px 0;background-color:#FFFFFF\">\r\n\r\n<div style=\"text-indent: 2em\">\r\n<div v-html=\"map.xianlujianjie\"></div></div>\r\n\r\n</div>        </el-tab-pane>\r\n            </el-tabs>\r\n</e-container>\r\n\r\n</div>    \r\n\r\n</e-container>\r\n</div>    </div>\r\n</template>\r\n<style type=\"text/scss\" scoped lang=\"scss\">\r\n    </style>\r\n<script>\r\n    import api from '@/api';\r\n    import { extend } from '@/utils/extend';\r\n        export default {\r\n        data() {\r\n            return {\r\n                loading:false,   // 加载\r\n                                isCollect:false, // 是否已经收藏\r\n                                map:{\r\n                        xianlubianhao:'',\r\n                        xianlumingcheng:'',\r\n                        tupian:'',\r\n                        chufadi:'',\r\n                        tujingdi:'',\r\n                        zhongdian:'',\r\n                        jiage:'',\r\n                        liulanliang:'',\r\n                        xianlutese:'',\r\n                        xianlujianjie:'',\r\n                        addtime:'',\r\n                            },\r\n                            }\r\n        },\r\n                props:{\r\n            id:{\r\n                type:[String,Number],\r\n                required:true\r\n            }\r\n        },\r\n        watch: {\r\n            id:{\r\n                handler(){\r\n                    this.loadDetail();\r\n                },\r\n                immediate:true\r\n            },\r\n                    },\r\n        computed: {\r\n                    },\r\n        methods: {\r\n            loadDetail(){\r\n                if(this.loading) return;\r\n                this.loading = true;\r\n                this.$get(api.lvyouxianlu.webdetail , {\r\n                    id:this.id\r\n                }).then(res=>{\r\n                    this.loading = false;\r\n                    if(res.code == api.code.OK){\r\n                        extend(this,res.data);\r\n                    }else{\r\n                        this.$message.error(res.msg);\r\n                    }\r\n                }).catch(err=>{\r\n                    this.loading = false;\r\n                    this.$message.error(err.message);\r\n                });\r\n            },\r\n\r\n                    },\r\n        created() {\r\n                    },\r\n        mounted() {\r\n                    },\r\n        destroyed() {\r\n                    }\r\n    }\r\n</script>\r\n"]}]}