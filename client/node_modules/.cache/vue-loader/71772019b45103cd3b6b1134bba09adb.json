       <span class=\"name\">\r\n                            附近景点：\r\n                        </span>\r\n                        <span class=\"val\">\r\n                            {{  map.fujinjingdian  }}                        </span>\r\n                    </div>\r\n                                    <div>\r\n                        <span class=\"name\">\r\n                            分类：\r\n                        </span>\r\n                        <span class=\"val\">\r\n                             <e-select-view module=\"meishifenlei\" :value=\"map.fenlei\" select=\"id\" show=\"fenleimingcheng\"></e-select-view>                        </span>\r\n                    </div>\r\n                                    <div>\r\n                        <span class=\"name\">\r\n                            人均价格：\r\n                        </span>\r\n                        <span class=\"val\">\r\n                            {{ map.jiage }}                        </span>\r\n                    </div>\r\n                                    <div>\r\n                        <span class=\"name\">\r\n                            美食简介：\r\n                        </span>\r\n                        <span class=\"val\">\r\n                            {{ map.meishijianjie }}                        </span>\r\n                    </div>\r\n                            </div>\r\n\r\n            \r\n            \r\n        </div>\r\n    </div>\r\n\r\n</div>\r\n\r\n</e-module-model-box>\r\n\r\n</div>    \r\n\r\n</e-container>\r\n</div>    </div>\r\n</template>\r\n<style type=\"text/scss\" scoped lang=\"scss\">\r\n    </style>\r\n<script>\r\n    import api from '@/api';\r\n    import { extend } from '@/utils/extend';\r\n        export default {\r\n        data() {\r\n            return {\r\n                loading:false,   // 加载\r\n                                map:{\r\n                        meishibianhao:'',\r\n                        mingcheng:'',\r\n                        fujinjingdian:'',\r\n                        fenlei:'',\r\n                        tupian:'',\r\n                        jiage:'',\r\n                        meishijianjie:'',\r\n                        addtime:'',\r\n                            },\r\n                            }\r\n        },\r\n                props:{\r\n            id:{\r\n                type:[String,Number],\r\n                required:true\r\n            }\r\n        },\r\n        watch: {\r\n            id:{\r\n                handler(){\r\n                    this.loadDetail();\r\n                },\r\n                immediate:true\r\n            },\r\n                    },\r\n        computed: {\r\n                    },\r\n        methods: {\r\n            loadDetail(){\r\n                if(this.loading) return;\r\n                this.loading = true;\r\n                this.$get(api.difangmeishi.webdetail , {\r\n                    id:this.id\r\n                }).then(res=>{\r\n                    this.loading = false;\r\n                    if(res.code == api.code.OK){\r\n                        extend(this,res.data);\r\n                    }else{\r\n                        this.$message.error(res.msg);\r\n                    }\r\n                }).catch(err=>{\r\n                    this.loading = false;\r\n                    this.$message.error(err.message);\r\n                });\r\n            },\r\n\r\n                    },\r\n        created() {\r\n                    },\r\n        mounted() {\r\n                    },\r\n        destroyed() {\r\n                    }\r\n    }\r\n</script>\r\n"]}]}